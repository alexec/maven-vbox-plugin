diff -ruN app0/Provisioning.xml app1/Provisioning.xml
--- app0/Provisioning.xml	1970-01-01 01:00:00.000000000 +0100
+++ app1/Provisioning.xml	2013-01-26 11:21:29.000000000 +0000
@@ -0,0 +1,26 @@
+<?xml version="1.0"?>
+<Provisioning xmlns="http://www.alexecollins.com/vbox/provisioning">
+    <Target name="install-os">
+        <Sleep seconds="10"/>
+        <KeyboardPutScanCodes keys="Esc"/>
+        <Sleep seconds="2"/>
+        <KeyboardPutScanCodes line="linux ks=floppy text"/>
+        <AwaitState state="poweroff" timeout="30 minutes"/>
+    </Target>
+    <Target name="run-post-install">
+        <Exec>vboxmanage storageattach %NAME% --storagectl "IDE Controller" --port 0 --device 0 --type dvddrive --medium "%VBOX_ADDITIONS%"</Exec>
+        <Exec failonerror="false">vboxmanage startvm %NAME%</Exec>
+        <Sleep minutes="1"/>
+        <KeyboardPutScanCodes>root
+keenbrick
+modprobe floppy
+mkdir /media/floppy
+mount /dev/fd0 /media/floppy
+/media/floppy/post-install.sh
+        </KeyboardPutScanCodes>
+        <AwaitState state="poweroff" timeout="30 minutes"/>
+    </Target>
+    <Target name="cleanup">
+        <Exec>vboxmanage storageattach %NAME% --storagectl "Floppy Controller" --port 0 --device 0 --medium none</Exec>
+    </Target>
+</Provisioning>
