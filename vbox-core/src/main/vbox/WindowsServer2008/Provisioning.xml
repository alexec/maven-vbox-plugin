<?xml version="1.0"?>
<Provisioning xmlns="http://www.alexecollins.com/vbox/provisioning">
    <Target name="install-os">
        <Sleep seconds="30"/>
        <KeyboardPutScanCodes keys="Tab,Tab,Tab,Enter"/>
        <AwaitState state="poweroff" timeout="20 minutes"/>
    </Target>
    <Target name="cleanup">
        <Exec>vboxmanage storageattach %NAME% --storagectl "IDE Controller" --port 0 --device 0 --medium none</Exec>
        <Exec>vboxmanage storageattach %NAME% --storagectl "IDE Controller" --port 0 --device 1 --medium none</Exec>
        <Exec>vboxmanage storageattach %NAME% --storagectl "Floppy Controller" -port 0 --device 0 --medium none</Exec>
    </Target>
</Provisioning>
