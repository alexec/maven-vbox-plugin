<?xml version="1.0"?>
<Provisions>
    <Sleep ms="5000"/>
    <KeyboardPutScanCodes keys="Esc,Esc,Enter"/>
    <Sleep ms="1000"/>
    <KeyboardPutScanCodes line="/install/vmlinuz noapic preseed/file=/floppy/preseed.cfg"/>
    <Sleep ms="60000"/>
    <!-- eject Ubunut ISO -->
    <Exec>vboxmanage storageattach %NAME% --name "IDE Controller" --medium none</Exec>
    <Exec>vboxmanage storageattach %NAME% --name "IDE Controller" %VBOX_ADDITIONS%</Exec>
    <!-- TODO - test -->
    <PortForward hostport="3022" guestport="22"/>
</Provisions>
